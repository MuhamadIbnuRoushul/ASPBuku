@model ASPBuku.Models.BukuModel

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    var stok = Model.Judul;
    if(stok == ""){
        <h3>Belum ada buku terbit</h3>
    }else{
        <h3>Judul buku yang terbit adalah : @Model.Judul</h3>
        <h4>Penerbit : @ViewBag.Penerbit</h4>
    }
}

